{
  "name": "@silica-protocol/explorer-models",
  "version": "0.1.0",
  "description": "Shared TypeScript domain models for Silica explorer frontends",
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./*": {
      "types": "./dist/src/*.d.ts",
      "default": "./dist/src/*.js"
    }
  },
  "types": "./dist/src/index.d.ts",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "clean": "node ./scripts/clean.mjs",
    "lint": "eslint \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "npm run lint && npm run test",
    "prepare": "npm run build"
  },
  "dependencies": {
    "typescript": "5.9.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "eslint": "8.57.1",
    "vitest": "2.1.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
